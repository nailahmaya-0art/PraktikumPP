#include <iostream>
#include <fstream>
using namespace std;

int main() {
    ifstream myFile;
    string semuaBaris;
    string satuBaris;

    myFile.open("data.txt");

    while (getline(myFile, satuBaris)) {
        semuaBaris += satuBaris + "\n";
    }

    cout << "Isi file 'data.txt' lebih dari satu baris:" << endl;
    cout << semuaBaris << endl;

    myFile.close();
    return 0;
}
